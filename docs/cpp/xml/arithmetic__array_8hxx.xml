        const ArrayExtender&lt;ARRAY_CLASS&gt; &amp; a, \
        const typename ArrayExtender&lt;ARRAY_CLASS&gt;::const_reference &amp; b \
    ){ \
        auto res = a; \
        for(auto i=0; i&lt;a.size(); ++i){ \
            res[i] inplaceSymbol b; \
        } \
