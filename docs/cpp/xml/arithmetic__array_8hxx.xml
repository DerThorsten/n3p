        <name>NIFTY_MACRO_BINARY_OP_INPLACE</name>
        <param><defname>operatorSymbol</defname></param>
        <initializer>template&lt;class ARRAY_CLASS&gt; \
    ArrayExtender&lt;ARRAY_CLASS&gt; &amp; operator operatorSymbol ( \
        ArrayExtender&lt;ARRAY_CLASS&gt; &amp; a, \
        const ArrayExtender&lt;ARRAY_CLASS&gt; &amp; b \
    ){ \
        <ref refid="runtime__check_8hxx_1a1ae39644615984f31785d52a0b624277" kindref="member">NIFTY_ASSERT_OP</ref>( a.size(),==,b.size()); \
        for(auto i=0; i&lt;a.size(); ++i){ \
            a[i] operatorSymbol b[i]; \
        } \
        return a; \
    } \
    template&lt;class ARRAY_CLASS&gt; \
    ArrayExtender&lt;ARRAY_CLASS&gt; &amp; operator operatorSymbol ( \
        ArrayExtender&lt;ARRAY_CLASS&gt; &amp; a, \
        typename ArrayExtender&lt;ARRAY_CLASS&gt;::const_reference  b \
    ){ \
        for(auto i=0; i&lt;a.size(); ++i){ \
            a[i] operatorSymbol b; \
        } \
        return a; \
    }</initializer>
