        <param><defname>inplaceSymbol</defname></param>
        <initializer>template&lt;class ARRAY_CLASS&gt; \
    ArrayExtender&lt;ARRAY_CLASS&gt;  operator operatorSymbol ( \
        const ArrayExtender&lt;ARRAY_CLASS&gt; &amp; a, \
        const ArrayExtender&lt;ARRAY_CLASS&gt; &amp; b \
    ){ \
        <ref refid="runtime__check_8hxx_1a1ae39644615984f31785d52a0b624277" kindref="member">NIFTY_ASSERT_OP</ref>( a.size(),==,b.size()); \
        auto res = a; \
        res inplaceSymbol b; \
        return res; \
    } \
